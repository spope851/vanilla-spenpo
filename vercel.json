{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "^/(.*\\.(js|css|ico|png|jpg|svg|woff2?|ttf))$",
      "dest": "/dist/$1"
    },
    {
      "src": "^/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ]
}