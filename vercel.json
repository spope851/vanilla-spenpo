{
  "routes": [
    {
      "src": "/api/([^/]+)/([^/]+)",
      "dest": "/api/$1/[id].js?id=$2"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1/index.js"
    },
    {
      "src": "^/(.*\\.(js|css|ico|png|jpg|svg|woff2?|ttf))$",
      "dest": "/$1"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
}